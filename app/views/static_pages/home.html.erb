<% if user_signed_in? && (current_user.resident_phone_number.blank? || current_user.callbox_phone_number.blank?) %>
  <%= render 'static_pages/gather_phone_numbers' %>
<% else %>
  <% if user_signed_in? %>
    <%= render 'static_pages/visitor_pass_form' %>
    <ol>
      <%= render @recent_visitor_passes %>
    </ol>
  <% end %>
<% end %>
